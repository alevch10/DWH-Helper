# Example environment variables for DWH Helper

# Application settings
TITLE="DWH Helper"
VERSION="1.0.0"
DESCRIPTION="Product analytics data ingestion and DWH integration"
DEBUG=false

# Logging
LOGGING_LEVEL=INFO

# DWH Database
# Connection string: sqlite:///./dwh.db or postgres://user:pass@host:port/dbname
DWH_DATABASE_URL="sqlite:///./dwh.db"
DWH_MAX_WRITE_BATCH_BYTES=10000000
DWH_MAX_ROWS_PER_INSERT=1000

# AppMetrica API
# Base URL for AppMetrica API (e.g., https://api.appmetrica.yandex.net)
APPMETRICA_BASE_URL="https://api.appmetrica.yandex.net"
# Your AppMetrica API key
APPMETRICA_API_KEY="your-api-key-here"
# Your AppMetrica application ID
APPMETRICA_APPLICATION_ID=123456789
# Polling settings
APPMETRICA_POLL_INTERVAL_SECONDS=5
APPMETRICA_POLL_TIMEOUT_SECONDS=300

# S3 Storage
# AWS credentials
S3_ACCESS_KEY_ID="your-access-key-here"
S3_SECRET_ACCESS_KEY="your-secret-key-here"
# AWS region (e.g., us-east-1, eu-west-1)
S3_REGION="us-east-1"
# S3 endpoint (leave empty for AWS, use custom URL for S3-compatible services)
S3_ENDPOINT_URL="https://s3.amazonaws.com"
# S3 bucket name
S3_BUCKET_NAME="your-bucket-name"
