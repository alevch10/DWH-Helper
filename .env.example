# Example environment variables for DWH Helper

# Application settings
TITLE="DWH Helper"
VERSION="1.0.0"
DESCRIPTION="Product analytics data ingestion and DWH integration"
DEBUG=false

# Auth
AUTH_READ_ACCESS=i.franko@google.com, t.shevchenko@google.com
AUTH_WRITE_ACCESS=l.ukrainka@google.com
# Logging
LOGGING_LEVEL=INFO

# DWH Database
# DWH Database
DWH_DATABASE_NAME="dwh"
DWH_DATABASE_USER="user"
DWH_DATABASE_PASSWORD="password"
DWH_DATABASE_HOST="localhost"
DWH_DATABASE_PORT=5432
DWH_MAX_PARAMS_PER_QUERY=65535
DWH_SAFETY_FACTOR=0.8

# AppMetrica API
# Base URL for AppMetrica API (e.g., https://api.appmetrica.yandex.net)
APPMETRICA_BASE_URL="https://api.appmetrica.yandex.net"
# Your AppMetrica application ID
APPMETRICA_APPLICATION_ID=123456789
# Polling settings
APPMETRICA_POLL_INTERVAL_SECONDS=5
APPMETRICA_POLL_TIMEOUT_SECONDS=300

# S3 Storage
# AWS credentials
S3_ACCESS_KEY_ID="your-access-key-here"
S3_SECRET_ACCESS_KEY="your-secret-key-here"
# AWS region (e.g., us-east-1, eu-west-1)
S3_REGION="us-east-1"
# S3 endpoint (leave empty for AWS, use custom URL for S3-compatible services)
S3_ENDPOINT_URL="https://s3.amazonaws.com"
# S3 bucket name
S3_BUCKET_NAME="your-bucket-name"

# Amplitude API
AMPLITUDE_WEB_SECRET_KEY="your-web-secret-key"
AMPLITUDE_WEB_CLIENT_ID="your-web-client-id"
AMPLITUDE_MOBILE_SECRET_KEY="your-mobile-secret-key"
AMPLITUDE_MOBILE_CLIENT_ID="your-mobile-client-id"

# Yandex OAuth
YANDEX_CLIENT_ID="fff"
YANDEX_CLIENT_SECRET="fff"